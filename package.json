{
  "name": "agora",
  "version": "0.1.0",
  "description": "An open forum for debates.",
  "main": "server.js",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "^0.0.3",
    "better-express-errors": "^1.0.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "http": "^0.0.0",
    "jsonwebtoken": "^8.1.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.2.5",
    "morgan": "^1.9.0",
    "npm-run-all": "^4.1.2",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "passport-twitter": "^1.0.4",
    "prop-types": "^15.6.0",
    "react-addons-css-transition-group": "^15.6.2",
    "socket.io": "^2.0.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "localtunnel": "^1.8.3",
    "mocha": "^5.2.0",
    "node-sass-chokidar": "^1.3.3",
    "nsp": "^3.1.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-materialize": "^2.3.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prestart": "echo \"Starting...\"",
    "start": "node server.js",
    "server": "nodemon server.js",
    "start-server": "npm-run-all -p prestart server",
    "start-react": "PORT=5000 react-scripts start",
    "start-all": "npm-run-all -p watch-css start-react start-server",
    "security": "nsp check",
    "localtunnel": "lt --port 8000",
    "share": "npm-run-all -p start-all localtunnel",
    "deploy": "echo \"Not Implemented Yet...\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maikelnabil/agora.git"
  },
  "keywords": [],
  "author": "Mike Nabil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maikelnabil/agora/issues"
  },
  "homepage": "http://agora.mikenabil.net/"
}
